#include <unistd.h>

int main(int argc, char **argv)
{
	int j;
	int i;

	i = 0;
	j = 0;
	if (argc == 3)
	{
		while(argv[1][i])
			while (argv[1][i] != argv[2][j] && argv[2][j] != '\0')
				j++;
			write(1, &argv[1][i], 1);
			i++;
	}
	write(1, "\n", 1);
}
